{
  "name": "fims",
  "version": "0.1.0",
  "private": true,
  "description": "fims backoffice",
  "keywords": [],
  "scripts": {
    "e2e": "protractor",
    "e2e-test": "protractor ./protractor.conf.js",
    "tslint": "tslint -c ./tslint.json \"./src/**/*.ts\" -e \"./src/**/typings.d.ts\" -e \"./src/environments/**\"",
